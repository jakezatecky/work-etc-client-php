{
	"name": "jakezatecky/work-etc-client",
	"description": "A thin HTTP client for WORK[etc]'s API.",
	"type": "library",
	"keywords": ["WORKetc", "API", "client"],
	"homepage": "https://github.com/jakezatecky/work-etc-client-php",
	"license": "MIT",
	"authors": [
		{
			"name": "Jake Zatecky"
		}
	],
	"support": {
		"issues": "https://github.com/jakezatecky/work-etc-client-php/issues",
		"source": "https://github.com/jakezatecky/work-etc-client-php"
	},
	"require": {
		"php": "^7.0",
		"ext-json": "*",
		"nategood/httpful": "^0.3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0.0",
		"mockery/mockery": "^0.9.9 || ^1.0.0"
	},
	"autoload": {
		"psr-4": {
